{
  "opts": {
    "destination": "./docs",
    "package": "./package.json",
    "readme": "./readme.md",
    "pedantic": true,
    "access": "public",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "source": {
    "include": ["./lib"]
  },
  "plugins": ["jsdoc-plugin-typescript"],
  "typescript": {
    "moduleRoot": "lib"
  },
  "templates": {
    "disablePackagePath": true
  }
}
