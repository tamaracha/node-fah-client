{
  "opts": {
    "destination": "./docs",
    "package": "./package.json",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "pedantic": true
  },
  "source": {
    "include": ["./lib"]
  },
  "plugins": [
    "jsdoc-plugin-typescript",
    "./node_modules/jsdoc/plugins/markdown"
  ],
  "typescript": {
    "moduleRoot": "lib"
  }
}
